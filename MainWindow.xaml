<Window x:Class="WSR6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WSR6"
        mc:Ignorable="d"
        Title="ООО «Цветы»" Height="500" Width="850" MinHeight="500" MinWidth="850">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="/Resources/logo.png" HorizontalAlignment="Left"></Image>
        <Image Grid.Row="1" Source="/Resources/logo.png" HorizontalAlignment="Center" Opacity="0.4"></Image>
        <TextBlock Text="ООО «Цветы»" HorizontalAlignment="Center" FontFamily="Comic Sans MS" FontSize="34" Margin="5"></TextBlock>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Background="#FF498C51" Content="Назад" HorizontalAlignment="Right" Margin="25" VerticalAlignment="Top" Width="88" Name="BtnBack" Click="BtnBack_Click"/>

    </Grid>
</Window>
